}
.tab-section .nav-tabs .nav-item.active:after{
	content: "";
    position: absolute;
    top: 112%;
    left: 50%;
    width: 0;
    height: 0;
    border-style: solid;
    border-width: 0 7.5px 11px 7.5px;
    border-color: transparent transparent #ced4da transparent;
    transform: translateX(-50%);
}
.banner .tab-section .nav-tabs .nav-item.active:after{
