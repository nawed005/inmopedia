.tab-section .nav-tabs .nav-link.active, .tab-section .nav-tabs .nav-link:hover, .tab-section .nav-tabs .nav-link:hover, .tab-section .nav-tabs .nav-item.active .nav-link:hover, .tab-section .nav-tabs .nav-item.active .nav-link:focus{
