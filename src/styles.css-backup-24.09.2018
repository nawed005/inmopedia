}
.tab-section .nav-tabs .nav-item{
	display:inline-block;
	float:none;
	vertical-align:middle;
	font-size:16px;
	position:relative;
}
.tab-section .nav-tabs .nav-item:before{
	content:"|";
	position:absolute;
	right:0;
	top:0;
}
.tab-section .nav-tabs .nav-link{
	background:none;
	font-size:16px;
	font-weight:500;
